		pinctrl-names = "default";
		pinctrl-0 = <&pinctrl_i2c2_lis331dlh>;
		/* status = "disabled"; */ /* TODO: ON */

		Vdd-supply = <&reg_3p3v>;
		Vdd_IO-supply = <&reg_3p3v>;
