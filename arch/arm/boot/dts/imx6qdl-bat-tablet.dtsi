		clocks = <&mipi_mclk>;
		clock-names = "csi_mclk";
		DOVDD-supply = <&reg_1p8v>;
		AVDD-supply = <&reg_2p5v>;
		DVDD-supply = <&reg_1p8v>;
		pwn-gpios = GP_OV5640_MIPI_1_POWER_DOWN;
		rst-gpios = GP_OV5640_MIPI_1_RESET;
		ipu_id = <0>;
		csi_id = <1>;
